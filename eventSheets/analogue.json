							"id": "set-position",
							"objectClass": "fanaStick",
							"sid": 497967720392639,
							"parameters": {
								"x": "(cos(cangle) * (dist < radius ? dist :  radius)) + fanaPad.X",
								"y": "(sin(cangle) * (dist < radius ? dist :  radius)) + fanaPad.Y"
							}
